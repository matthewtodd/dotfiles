[alias]
	out = !git log --boundary $(git rev-list --max-count=1 --simplify-by-decoration HEAD^)..
	st = status
[color]
	ui = auto
[core]
	excludesfile = ~/.gitignore
[format]
	pretty = format:%Cgreen%h%Creset%C(yellow)%d%Creset %s %C(cyan)%ar%Creset
